{
  "name": "teach-niche",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx next dev",
    "clean": "rm -rf .next",
    "build": "npm run clean && npx next build",
    "start": "npx next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx playwright test --config=e2e-tests/playwright.config.ts",
    "test:visual": "PLAYWRIGHT_TEST_BASE_URL=http://localhost:3000 npx playwright test --project='Visual Tests' --config=e2e-tests/playwright.config.ts",
    "test:visual:update": "UPDATE_SNAPSHOTS=1 npm run test:visual",
    "type-check": "tsc --noEmit",
    "rls:migrate": "ts-node scripts/run-rls-migration.ts",
    "playwright": "playwright test --config=e2e-tests/playwright.config.ts",
    "find:unused": "next-unused",
    "verify-unused": "bash ./cleanup-script.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.0",
    "@mux/mux-node": "^9.0.1",
    "@mux/mux-player-react": "^3.3.0",
    "@mux/mux-uploader-react": "^1.1.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.48.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@uiw/react-md-editor": "^4.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "motion": "^12.4.2",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "rehype-sanitize": "^6.0.0",
    "stripe": "^17.6.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "next-unused": {
    "debug": true,
    "include": [
      "app"
    ],
    "exclude": [
      "node_modules",
      ".next",
      "public"
    ],
    "entrypoints": [
      "app/page.tsx"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.50.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.6.0",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "20.17.23",
    "@types/react": "19.0.10",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-themes": "^0.4.4",
    "next-unused": "^0.0.6",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2",
    "wait-on": "^8.0.2",
    "whatwg-fetch": "^3.6.20"
  }
}
